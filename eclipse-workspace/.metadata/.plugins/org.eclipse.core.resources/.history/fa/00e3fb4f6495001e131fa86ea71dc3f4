package br.com.senaisp.bauru.richard.secao01;

public class TiposDeDados {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		boolean tBooleano = true; //true e false 1bit
		byte tByte = -128; //-128 até 127 byte
		char tChar = 'A';//65
		//Inteiros
		short tShort = 152;//-32.768 a 32.767
		int tInt = 454; //-2.147.483.648 a 2.147.483.647
		//-9.223.372.036.854.775.808 a 
		// 9.223.372.036.854.775.807
		long tLong = 2_147_483_648L;
		//Decimais
		//1.4023x10-45 a 3.4028x10+38
		float tFloat = 15.3F;
		//4.9406x10 -324 a 1.7977x10 308
		double tDouble = 16.55;
		//Demonstrando valores binarios
		int tValorBinario = 0b10;// igual a 2
		int tValorHexa = 0xCAFE_BABE;
	}

}
